@model Hospital

<a asp-controller="Hospitales"
   asp-action="Index">
    Volver al listado
</a>
<hr/>
<ul class="list-group">
    <li class="list-group-item">
        Id Hospital: @Model.IdHospital
    </li>
    <li class="list-group-item">
        Nombre: @Model.Nombre
    </li>
    <li class="list-group-item">
        Dirección: @Model.Direccion
    </li>
    <li class="list-group-item-danger">
        Teléfono: @Model.Telefono
    </li>
</ul>